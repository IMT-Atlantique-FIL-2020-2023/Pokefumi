// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./stats.sqlite"
}

model StatRound {
  idPokemon Int
  dateMatch DateTime
  idMatch   Int
  victory   Boolean
  team      Int

  @@id([idPokemon, dateMatch, idMatch, team])
}
